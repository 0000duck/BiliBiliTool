{
  //日志
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Debug", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": { "restrictedToMinimumLevel": "Information" }
      },
      { "Name": "Debug" },
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },

  //程序自定义个性化配置
  "DailyTaskConfig": {
    "NumberOfCoins": 5, //每日设定的投币数 [0,5]
    "SelectLike": false, //投币时是否同时点赞[false,true]
    "MonthEndAutoCharge": true, //年度大会员自动充电[false,true]
    "DevicePlatform": "ios" //执行客户端操作时的平台 [ios,android]
  },

  //Cookie信息,取自浏览器,必填.优先级低于命令行中的配置值
  "BiliBiliCookies": {
    "UserId": "", //你的DEDEUSERID
    "SessData": "", //你的SESSDATA
    "BiliJct": "" //你的BILI_JCT
  }
}
