{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Debug", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": { "restrictedToMinimumLevel": "Information" }
      },
      { "Name": "Debug" },
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },

  "DailyTaskConfig": {
    "NumberOfCoins": 5,
    "SelectLike": 0,
    "MonthEndAutoCharge": true,
    "DevicePlatform": "ios"
  },

  "BiliBiliCookies": {
    "UserId": "", //你的DEDEUSERID,取自Cookie,必填
    "SessData": "", //你的SESSDATA,取自Cookie,必填
    "BiliJct": "" //你的BILI_JCT,取自Cookie,必填
  }
}
